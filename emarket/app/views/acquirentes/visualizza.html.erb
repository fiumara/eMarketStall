<h1><%= t_google("Dati Acquirente") %></h1>

<p><strong><%= t_google("Username:") %></strong> <%= @acquirente.nome_utente %></p>

<% unless @profilo_limitato %>
  <p><strong><%= t_google("Nome:") %></strong> <%= @acquirente.nome %></p>
  <p><strong><%= t_google("Cognome:") %></strong> <%= @acquirente.cognome %></p>
  <p><strong><%= t_google("Email:") %></strong> <%= @acquirente.email %></p>
  <p><strong><%= t_google("Telefono:") %></strong> <%= @acquirente.telefono %></p>
<% else %>
  <p><em><%= t_google("Profilo privato: i dati personali non sono visibili.") %></em></p>
<% end %>

